/* .Fortran calls */
extern void F77_NAME(droughtindicator)(void *, void *, void *, void *, void *);

static const R_FortranMethodDef FortranEntries[] = {
    {"droughtindicator", (DL_FUNC) &F77_NAME(droughtindicator), 5},
    {NULL, NULL, 0}
};

void R_init_SMI2(DllInfo *dll)
{
    R_registerRoutines(dll, NULL, NULL, FortranEntries, NULL);
    R_useDynamicSymbols(dll, FALSE);
}
